{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "mapRoot": "./",
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es2015",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "@abp/*": [
        "../node_modules/abp-ng2-module/dist/src/*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@account/*": [
        "./account/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@node_modules/*": [
        "../node_modules/*"
      ],
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "@metronic/*": [
        "./assets/metronic/*"
      ],
      "@app_pages/*": [
        "./app/pages"
      ],
      "@app_components/*": [
        "./app/components"
      ],
      "@assets/*": [
        "./assets/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
